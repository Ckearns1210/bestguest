<%= form_for @party do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name%><br>
  <%= f.label :location %><br>
  <%= f.text_field :location %><br>
  <%= f.label :date %><br>
  <%= f.date_field :date %><br>
  <%= f.label :time %><br>
  <%= f.time_field :time %><br>
  <%= f.label :img_url %><br>
  <%= f.text_field :img_url %><br>
  <%= f.collection_select(:user_id, User.all, :id, :user_name ) %>
  <%= f.submit %>


<% end %>
